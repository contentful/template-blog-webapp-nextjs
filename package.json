    "setup": "./bin/setup.sh"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "src/**/*.{json,js,ts,jsx,tsx}": [
      "prettier --write"
    ]
  },
  "license": "MIT",
