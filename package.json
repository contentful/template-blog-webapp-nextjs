    "@next/bundle-analyzer": "14.2.29",
    "dotenv": "16.5.0",
    "graphql": "16.11.0",
    "i18next-browser-languagedetector": "8.1.0",
    "i18next-resources-to-backend": "1.2.1",
    "next": "14.2.29",
    "next-compose-plugins": "2.2.1",
    "next-i18n-router": "5.5.2",
    "react": "18.3.1",
